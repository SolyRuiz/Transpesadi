<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data>
		<record id="fleet_repair_checklist_paperformat" model="report.paperformat">
			<field name="name">Car Repair Checklist A4</field>
			<field name="default" eval="True" />
			<field name="format">A4</field>
			<field name="page_height">0</field>
			<field name="page_width">0</field>
			<field name="orientation">Portrait</field>
			<field name="margin_top">60</field>
			<field name="margin_bottom">30</field>
			<field name="margin_left">7</field>
			<field name="margin_right">10</field>
			<field name="header_line" eval="False"/>
			<field name="header_spacing">50</field>
			<field name="dpi">90</field>
		</record>		

        <record id="fleet_repair_checklist_id" model="ir.actions.report">
            <field name="name">Car Repair Checklist</field>
            <field name="model">fleet.repair</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">car_repair_industry.machi_repa_checklist_temp_id</field>
            <field name="report_file">car_repair_industry.machi_repa_checklist_temp_id</field>
            <field name="binding_type">report</field>
            <field name="binding_model_id" ref="model_fleet_repair"/>
            <field name="paperformat_id" ref="car_repair_industry.fleet_repair_checklist_paperformat"/>

        </record>
		
	</data>
</odoo>
